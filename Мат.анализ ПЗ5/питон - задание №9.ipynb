        "from sympy import *\n",
        "from math import radians\n",
        "sinus = 0\n",
        "cosinus = 1\n",
        "\n",
        "for i in range(1,31):\n",
        "  x = radians(1)\n",
        "  sinus = sinus + cosinus * x\n",
        "  cosinus = (1 - sinus**2)**(1/2)\n",
        "  print(f'синус угла {i} градус(ов) равен {sinus}. косинус угла {i} градус(ов) равен {cosinus}.')\n",
